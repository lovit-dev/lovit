{
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "strict": true,
    "noEmit": true,
    "stripInternal": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  },
  "include": ["src"]
}
